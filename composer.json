{
  "name": "simbiosis/whatsapp-number-validator",
  "description": "A PHP package for validating WhatsApp numbers",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Simbiosis",
      "email": "info@simbiosis.team"
    }
  ],
  "version": "1.0.0",
  "autoload": {
    "psr-4": {
      "Simbiosis\\WhatsAppNumberValidator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Simbiosis\\WhatsAppNumberValidator\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.2|^11.0|^12.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0 || ^11.0 || ^12.0",
    "orchestra/testbench": "^8.0 || ^9.0 || ^10.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Simbiosis\\WhatsAppNumberValidator\\WhatsAppNumberValidatorServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
